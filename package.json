{
  "name": "i18ner",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "dev": "vite --host 0.0.0.0",
    "build": "vite build",
    "preview": "vite preview --host 0.0.0.0",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "fmt:check": "yarn prettier --check ./**/*.{js,ts,svelte,css} !./dist/**/*",
    "fmt:fix": "yarn prettier --write ./**/*.{js,ts,svelte,css} !./dist/**/*",
    "lint:check": "yarn eslint --ext .js,.ts,.svelte src",
    "lint:fix": "yarn eslint --fix --ext .js,.ts,.svelte src"
  },
  "devDependencies": {
    "@sveltejs/vite-plugin-svelte": "^1.0.0-next.30",
    "@tsconfig/svelte": "^2.0.1",
    "@typescript-eslint/eslint-plugin": "^5.29.0",
    "@typescript-eslint/parser": "^5.29.0",
    "bootstrap-icons": "^1.8.3",
    "eslint": "^8.18.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-svelte3": "^4.0.0",
    "prettier": "^2.7.1",
    "prettier-plugin-svelte": "^2.7.0",
    "sass": "^1.53.0",
    "svelte": "^3.44.0",
    "svelte-check": "^2.2.7",
    "svelte-preprocess": "^4.9.8",
    "svelte-spa-router": "^3.2.0",
    "sveltestrap": "^5.9.0",
    "tslib": "^2.3.1",
    "typescript": "^4.5.4",
    "vite": "^2.9.9"
  },
  "dependencies": {
    "bootstrap": "^5.1.3",
    "jszip": "^3.10.0"
  }
}
